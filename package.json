{
  "name": "bot-template",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "register": "tsx src/register.ts",
    "register:release": "tsx src/register.ts --release",
    "dev": "tsx src/index.ts",
    "lint": "oxlint src",
    "lint:fix": "oxlint src --fix",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.1.0",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3"
  }
}
